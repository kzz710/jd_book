<div class="userInfo">

    <div class="w">
        <h1>收货地址</h1>
        <div class="addAddress">
            <h2>新增收货地址</h2>
            <form action="" id="AddressForm">
                <input type="hidden" name="Id" id="Id">
                <input type="hidden" name="UserId" value="@user.Id" id="UserId"/>
                <div class="msg">
                    <label for="Name">收货人姓名：</label>
                    <input type="text" name="Name" id="Name">
                </div>
                <div class="msg">
                    <label for="ZipCode">邮政编码：</label>
                    <input type="text" name="ZipCode" id="ZipCode">
                </div>
                <div class="msg">
                    <label for="Telphone">手机号码：</label>
                    <input type="text" name="Telphone" id="Telphone">
                </div>
                <div class="msg">
                    <label for="Address">收货地址：</label>
                    <textarea id="Address" name="Address"></textarea>
                </div>
            </form>
            <button id="saveBtn">保存</button>
        </div>
        <div class="haveAddress">
            <h2>已有收货地址</h2>
            @{
                
                List<AddressInfo> list=ViewBag.AddressList;
                if (list==null)
                {
                    <div class="no-address">
                        <i></i>
                        <div class="msg">您还未添加收货地址</div>
                    </div>
                }else
                {
                    <div class="have-address">
                        <table>
                            <thead>
                                <tr>
                                    <td></td>
                                    <td>收货人</td>
                                    <td>地址</td>
                                    <td>电话</td>
                                    <td>邮编</td>
                                    <td>操作</td>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (AddressInfo addressInfo in list)
                                {
                                    <tr>
                                        <td>
                                            <input type="radio" name="AddressId" value="@addressInfo.Id" class="addressId">
                                        </td>
                                        <td class="addressName">@addressInfo.Name</td>
                                        <td class="address-Info">@addressInfo.Address</td>
                                        <td class="addressTel">@addressInfo.Telphone</td>
                                        <td class="addressZC">@addressInfo.ZipCode</td>
                                        <td>
                                            <a href="javascript:void(0)" id="update">修改</a>
                                            <a href="javascript:void(0)" id="delete">删除</a>
                                        </td>
                                    </tr>
                                }
                                
                            </tbody>
                            <tfoot>
                                <tr>
                                    <td colspan="6">
                                       
                                    </td>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                    }
           
          
            }
        </div>
    </div>
</div>


